####################################### loss function


layer {
  bottom: "cls_pred_small"
  bottom: "proposal_labels_small"
  bottom: "roi_num"
  top: "loss_cls_small"
  name: "loss_cls_small"
  type: "SoftmaxWithLoss2"
  softmax_split_param {
    branch_id: 1 # small:1, large:2
  }
  loss_weight: 1
  propagate_down: 1
  propagate_down: 0
  propagate_down: 0
}

layer {
  name: "cls_accuracy_small"
  type: "Accuracy"
  bottom: "cls_pred_small"
  bottom: "proposal_labels_small"
  top: "cls_accuracy_small"
