#=================== detection subnet ======================
layer {
  name: "ROISplit" #use ./data/kitti/statistical_size.m to determine the parameter
  type: "ROISplit"
  bottom: "rois"
  top: "roi_num"
  top: "hash_table"
  roi_split_param {
    branch_num: 2
    split_area1: 1664 
    fluctuation_range_large: 157 
    fluctuation_range_small: 157  
  }
}

layer {
  name: "roi_pool_2"
  type: "CAROIPooling"
  bottom: "conv2_3"
  bottom: "rois"
  top: "roi_pool_2"
  caroi_pooling_param {
    pooled_w: 6
    pooled_h: 6
    spatial_scale: 0.25 # 1/4
    pad_ratio: 0
  }
}

layer {
  name: "roi_pool_3"
  type: "CAROIPooling"
  bottom: "conv3_4"
  bottom: "rois"
  top: "roi_pool_3"
  caroi_pooling_param {
    pooled_w: 6
    pooled_h: 6
    spatial_scale: 0.125 # 1/8
    pad_ratio: 0
  }
}

layer {
  name: "roi_pool_4"
  type: "CAROIPooling"
  bottom: "conv4_4"
  bottom: "rois"
  top: "roi_pool_4"
  caroi_pooling_param {
    pooled_w: 6
    pooled_h: 6
    spatial_scale: 0.0625 # 1/16
    pad_ratio: 0
  }
}

layer {
  name: "roi_pool_5"
  type: "CAROIPooling"
  bottom: "conv5_4"
  bottom: "rois"
  top: "roi_pool_5"
  caroi_pooling_param {
    pooled_w: 6
    pooled_h: 6
    spatial_scale: 0.03125 # 1/32
    pad_ratio: 0
  }
}

layer {         
  name: "roi_split"
  bottom: "roi_pool_2" # pooled features
  bottom: "roi_num"        # get from "ROISplit" layer
  bottom: "hash_table"     # get from "ROISplit" layer
  top: "roi_small_2"         # features of RoIs for small branch
  top: "roi_large_2"         # features of RoIs for large branch
  type: "ROIFeatureSplit"
  roi_feature_split_param {
    branch_num: 2
  }
}

layer {         
  name: "roi_split"
  bottom: "roi_pool_3" # pooled features
  bottom: "roi_num"        # get from "ROISplit" layer
  bottom: "hash_table"     # get from "ROISplit" layer
  top: "roi_small_3"         # features of RoIs for small branch
  top: "roi_large_3"         # features of RoIs for large branch
  type: "ROIFeatureSplit"
  roi_feature_split_param {
    branch_num: 2
  }
}


layer {         
  name: "roi_split"
  bottom: "roi_pool_4" # pooled features
  bottom: "roi_num"        # get from "ROISplit" layer
  bottom: "hash_table"     # get from "ROISplit" layer
  top: "roi_small_4"         # features of RoIs for small branch
  top: "roi_large_4"         # features of RoIs for large branch
  type: "ROIFeatureSplit"
  roi_feature_split_param {
    branch_num: 2
  }
}


layer {         
  name: "roi_split"
  bottom: "roi_pool_5" # pooled features
  bottom: "roi_num"        # get from "ROISplit" layer
