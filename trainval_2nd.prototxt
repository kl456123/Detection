  roi_feature_split_param {
    branch_num: 2
  }
}

################################################################concat layer
layer {
  name: "concat_small"
  bottom: "roi_small_2"
  bottom: "roi_small_3"
  bottom: "roi_small_4"
  bottom: "roi_small_5"
  top: "concat_small"
  type: "Concat"
  concat_param {
    axis: 1
  }
}

layer {
  name: "concat_large"
  bottom: "roi_large_2"
  bottom: "roi_large_3"
  bottom: "roi_large_4"
  bottom: "roi_large_5"
